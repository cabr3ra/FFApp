<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="wrap_content"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools">

    <TextView
        android:id="@+id/tv_rv_fruitShopName"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        tools:text="Aqui el name fruitShop"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintTop_toTopOf="parent"/>

    <TextView
        android:id="@+id/tv_rv_fruitShopLocation"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        tools:text="Aqui el location fruitShop"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/tv_rv_fruitShopName"/>

    <TextView
        android:id="@+id/tv_rv_fruitShopPhone"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        tools:text="Aqui el phone fruitShop"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/tv_rv_fruitShopLocation"/>

</androidx.constraintlayout.widget.ConstraintLayout>